{
     "framework": "create-react-app",
     "buildCommand": "npm run build",
     "outputDirectory": "build",
     "regions": ["iad1"],
     "env": {
       "NODE_ENV": "production"
     },
     "github": {
       "silent": true,
       "autoAlias": true
     },
     "headers": [
       {
         "source": "/(.*)",
         "headers": [
           {
             "key": "X-Content-Type-Options",
             "value": "nosniff"
           },
           {
             "key": "X-Frame-Options",
             "value": "DENY"
           },
           {
             "key": "X-XSS-Protection",
             "value": "1; mode=block"
           }
         ]
       },
       {
         "source": "/static/(.*)",
         "headers": [
           {
             "key": "Cache-Control",
             "value": "public, max-age=31536000, immutable"
           }
         ]
       }
     ]
   }
   